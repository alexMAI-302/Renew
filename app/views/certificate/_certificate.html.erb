<% if !@res.nil? %>
	<table width="100%" border="0" cellpadding="2" cellspacing="1">
		<% @res.each do |cert| %>
			<tr>
				<td>
					<%= image_tag "/certificate/get_certificate?inn=#{@inn}&ndoc=#{@ndoc}&id=#{cert['id']}", :alt=>"#{cert['goods_name']}" %>
				<td>
			</tr>
		<% end %>
	</table>
<% else %>
	По вашему запрсу ничего не найдено
<% end %>