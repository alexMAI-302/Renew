<% if !@res.nil? %>
	<table width="100%" border="0" cellpadding="2" cellspacing="1">
		<% @res.each do |cert| %>
			<tr>
				<td>
					<a target="blank" href="<%= "/images/certificates/#{cert['id']}.jpg" %>"> <%= cert['goods_name'] %> </a>
				<td>
			</tr>
		<% end %>
	</table>
<% else %>
	По вашему запрсу ничего не найдено
<% end %>